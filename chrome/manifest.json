  {
    "name": "terminal",
    "version": "0.0.0",
    "description": "foobar",

    "incognito": "spanning",
    "permissions": [
        "nativeMessaging",
        "contextMenus"
    ],

    "options_page": "options.html",
    "background": {
        "scripts": [
            "util.js",
            "background.js"
        ]
    },

    "page_action": {
        "default_title": "terminal",
        "default_popup": "popup.html"
    },

    "icons": {
        "16": "logo16.png",
        "32": "logo32.png",
        "128": "logo128.png",
        "512": "logo512.png"
    },
    "manifest_version": 2
}
