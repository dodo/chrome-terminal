  {
    "name": "terminal",
    "version": "0.0.0",
    "description": "foobar",

    "app": {
        "launch": {
            "local_path": "terminal.html"
        }
    },

    "permissions": [
        "nativeMessaging",
        "contextMenus"
    ],

    "options_page": "options.html",
    "background": {
        "scripts": [
            "util.js",
            "background.js"
        ]
    },

    "icons": {
        "16": "logo16.png",
        "32": "logo32.png",
        "128": "logo128.png",
        "512": "logo512.png"
    },
    "manifest_version": 2
}
